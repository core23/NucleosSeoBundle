{
    "name": "nucleos/seo-bundle",
    "description": "Symfony NucleosSeoBundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "seo",
        "sonata"
    ],
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "homepage": "https://nucleos.rocks",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/core23"
        },
        {
            "type": "opencollective",
            "url": "https://opencollective.com/core23"
        },
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/core23"
        },
        {
            "type": "other",
            "url": "https://donate.core23.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "sonata-project/block-bundle": "^4.0",
        "sonata-project/exporter": "^2.0 || ^3.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4.13 || ^5.4 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.4 || ^6.0",
        "twig/twig": "^2.9 || ^3.0"
    },
    "require-dev": {
        "knplabs/knp-menu": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/filesystem": "^4.4 || ^5.4 || ^6.0",
        "symfony/finder": "^4.4 || ^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nucleos\\SeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nucleos\\SeoBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
